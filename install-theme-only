#!/bin/bash
url=https://raw.githubusercontent.com/Putra-0/theme-xderm-putra/main
mkdir /www/xderm/img
chmod 777 /www/xderm/img
wget -O /www/xderm/img/ico.png --no-check-certificate $url/xderm/img/ico.png
wget -O /www/xderm/img/image.png --no-check-certificate $url/xderm/img/image.png
wget -O /www/xderm/img/background.jpg --no-check-certificate $url/xderm/img/background.jpg
wget -O /www/xderm/index.php --no-check-certificate $url/xderm/index.php
wget -O /www/xderm/api.php --no-check-certificate $url/xderm/api.php
mkdir /www/xderm/css
chmod 777 /www/xderm/css
wget -O /www/xderm/css/style.css --no-check-certificate $url/xderm/css/style.css
wget -O /www/xderm/css/theme.css --no-check-certificate $url/xderm/css/theme.css
clear;echo "Install Selesai, Silahkan clear cache browser!"
